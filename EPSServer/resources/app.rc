1 VERSIONINFO
FILEVERSION     @EPS_SERVER_PRODUCT_VERSION_MAJOR@,@EPS_SERVER_PRODUCT_VERSION_MINOR@,@EPS_SERVER_PRODUCT_VERSION_PATCH@,0
PRODUCTVERSION  @EPS_SERVER_PRODUCT_VERSION_MAJOR@,@EPS_SERVER_PRODUCT_VERSION_MINOR@,@EPS_SERVER_PRODUCT_VERSION_PATCH@,0
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4"
        BEGIN
            VALUE "CompanyName", "@EPS_PRODUCT_COMPANY_NAME@"
            VALUE "FileDescription", "@EPS_SERVER_PRODUCT_DESCRIPTION@"
            VALUE "FileVersion", "@EPS_SERVER_PRODUCT_VERSION@"
            VALUE "InternalName", "@EPS_SERVER_PRODUCT_INTERNAL_NAME@"
            VALUE "LegalCopyright", "@EPS_PRODUCT_COPYRIGHT@"
            VALUE "OriginalFilename", "@EPS_SERVER_BIN_NAME@.exe"
            VALUE "ProductName", "@EPS_SERVER_PRODUCT_NAME@"
            VALUE "ProductVersion", "@EPS_SERVER_PRODUCT_VERSION@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END
2 ICON "app-icon.ico"
